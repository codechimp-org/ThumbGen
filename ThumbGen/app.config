            </setting>
            <setting name="MainWindowState" serializeAs="String">
                <value>Normal</value>
            </setting>
            <setting name="ResizeExactlyHeight" serializeAs="String">
                <value>72</value>
            </setting>
            <setting name="ResizeExactlyWidth" serializeAs="String">
                <value>92</value>
            </setting>
            <setting name="ResizeWidths" serializeAs="String">
                <value>92</value>
            </setting>
            <setting name="ResizeHeights" serializeAs="String">
                <value>72</value>
            </setting>
            <setting name="FileType" serializeAs="String">
                <value>0</value>
            </setting>
            <setting name="ResizeResolution" serializeAs="String">
